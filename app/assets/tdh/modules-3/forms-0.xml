<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Add Tablet User</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/97B3CE6E-A534-42BD-A17F-76F7DDB8D8AE" uiVersion="1" version="687" name="Add Tablet User">
					<no_admin_warning/>
					<admin_authentication>
						<entered_admin_pin/>
					</admin_authentication>
					<registration_question>
						<username/>
						<duplicate_username_message/>
						<entered_pin_1/>
						<entered_pin_2/>
						<qualification/>
						<user_phone_number/>
						<come_from_another_clinic/>
					</registration_question>
					<previous_clinic_region/>
					<previous_clinic_province/>
					<previous_clinic_department/>
					<previous_clinic_cpss/>
					<security_questions>
						<security_question_explain_message/>
						<user_dob/>
						<mother_first_name/>
					</security_questions>
					<confirmation>
						<user_creation_confirmation_message/>
						<admin_creation_confirmation/>
					</confirmation>
					<debug>
						<debug_admin_user/>
						<debug_admin_pin/>
						<debug_admin_exists/>
					</debug>
					<user_type/>
					<admin_username/>
					<admin_pin/>
					<does_admin_exist/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>tablet_user</case_type></create><update><mother_first_name/><pin/><qualification/><user_dob/><user_phone_number/><user_type/><username/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<instance id="casedb" src="jr://instance/casedb"/>
			<instance src="jr://fixture/commtrack:locations" id="locations"/>
			<bind nodeset="/data/no_admin_warning" relevant="/data/does_admin_exist = 'no'" required="true()"/>
			<bind nodeset="/data/admin_authentication" relevant="/data/does_admin_exist = 'yes'"/>
			<bind nodeset="/data/admin_authentication/entered_admin_pin" type="xsd:string" constraint=". = /data/admin_pin" jr:constraintMsg="jr:itext('admin_authentication/entered_admin_pin-constraintMsg')" required="true()"/>
			<bind nodeset="/data/registration_question"/>
			<bind nodeset="/data/registration_question/username" type="xsd:string" required="true()"/>
			<bind nodeset="/data/registration_question/duplicate_username_message" constraint="1=2" jr:constraintMsg="jr:itext('registration_question/duplicate_username_message-constraintMsg')" relevant="/data/registration_question/username != '' and (count(instance('casedb')/casedb/case[@case_type='tablet_user'][username =/data/registration_question/username]) != 0)" required="true()"/>
			<bind nodeset="/data/registration_question/entered_pin_1" type="xsd:string" constraint="string-length(.) = 4 and regex(.,'^[0-9]+$')" jr:constraintMsg="jr:itext('registration_question/entered_pin_1-constraintMsg')" required="true()"/>
			<bind nodeset="/data/registration_question/entered_pin_2" type="xsd:string" constraint=". = /data/registration_question/entered_pin_1" jr:constraintMsg="jr:itext('registration_question/entered_pin_2-constraintMsg')" required="true()"/>
			<bind nodeset="/data/registration_question/qualification" required="true()"/>
			<bind nodeset="/data/registration_question/user_phone_number" type="xsd:string"/>
			<bind nodeset="/data/registration_question/come_from_another_clinic" required="true()"/>
			<bind nodeset="/data/previous_clinic_region" relevant="/data/registration_question/come_from_another_clinic = 'yes'" required="true()"/>
			<bind nodeset="/data/previous_clinic_province" relevant="/data/previous_clinic_region != ''" required="true()"/>
			<bind nodeset="/data/previous_clinic_department" relevant="/data/previous_clinic_province != ''" required="true()"/>
			<bind nodeset="/data/previous_clinic_cpss" relevant="/data/previous_clinic_department != ''" required="true()"/>
			<bind nodeset="/data/security_questions"/>
			<bind nodeset="/data/security_questions/security_question_explain_message"/>
			<bind nodeset="/data/security_questions/user_dob" type="xsd:date" constraint=". &lt; today()" jr:constraintMsg="jr:itext('security_questions/user_dob-constraintMsg')" required="true()"/>
			<bind nodeset="/data/security_questions/mother_first_name" type="xsd:string" constraint="regex(.,'^[^\s]+$')" jr:constraintMsg="jr:itext('security_questions/mother_first_name-constraintMsg')" required="true()"/>
			<bind nodeset="/data/confirmation"/>
			<bind nodeset="/data/confirmation/user_creation_confirmation_message"/>
			<bind nodeset="/data/confirmation/admin_creation_confirmation" relevant="/data/does_admin_exist = 'no'"/>
			<bind nodeset="/data/debug" relevant="1=2"/>
			<bind nodeset="/data/debug/debug_admin_user"/>
			<bind nodeset="/data/debug/debug_admin_pin"/>
			<bind nodeset="/data/debug/debug_admin_exists"/>
			<bind nodeset="/data/user_type" calculate="if(/data/does_admin_exist='yes', 'regular', 'admin')"/>
			<bind nodeset="/data/admin_username" calculate="instance('casedb')/casedb/case[@case_type='tablet_user'][@status='open'][user_type = 'admin'][1]/username"/>
			<bind nodeset="/data/admin_pin" calculate="instance('casedb')/casedb/case[@case_type='tablet_user'][@status='open'][user_type = 'admin'][1]/pin"/>
			<bind nodeset="/data/does_admin_exist" calculate="if(count(instance('casedb')/casedb/case[@case_type='tablet_user'][@status='open'][user_type = 'admin']) &gt; 0, 'yes', 'no')"/>
			<itext>
				<translation lang="en" default="">
