		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><setvalue event="xforms-ready" ref="/data/case/@case_id" value="instance('commcaresession')/session/data/case_id_new_tablet_user_0"/><bind calculate="/data/registration_question/username" nodeset="/data/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/case/create/owner_id"/><bind calculate="/data/security_questions/mother_first_name" nodeset="/data/case/update/mother_first_name" relevant="count(/data/security_questions/mother_first_name) &gt; 0"/><bind calculate="/data/registration_question/entered_pin_1" nodeset="/data/case/update/pin" relevant="count(/data/registration_question/entered_pin_1) &gt; 0"/><bind calculate="/data/registration_question/qualification" nodeset="/data/case/update/qualification" relevant="count(/data/registration_question/qualification) &gt; 0"/><bind calculate="/data/security_questions/user_dob" nodeset="/data/case/update/user_dob" relevant="count(/data/security_questions/user_dob) &gt; 0"/><bind calculate="/data/registration_question/user_phone_number" nodeset="/data/case/update/user_phone_number" relevant="count(/data/registration_question/user_phone_number) &gt; 0"/><bind calculate="/data/user_type" nodeset="/data/case/update/user_type" relevant="count(/data/user_type) &gt; 0"/><bind calculate="/data/registration_question/username" nodeset="/data/case/update/username" relevant="count(/data/registration_question/username) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<trigger ref="/data/no_admin_warning">
			<label ref="jr:itext('no_admin_warning-label')"/>
		</trigger>
		<group ref="/data/admin_authentication" appearance="field-list">
			<label ref="jr:itext('admin_authentication-label')"/>
			<secret ref="/data/admin_authentication/entered_admin_pin" appearance="numeric">
				<label ref="jr:itext('admin_authentication/entered_admin_pin-label')"/>
			</secret>
		</group>
		<group ref="/data/registration_question" appearance="field-list">
			<label ref="jr:itext('registration_question-label')"/>
			<input ref="/data/registration_question/username">
				<label ref="jr:itext('registration_question/username-label')"/>
			</input>
			<trigger ref="/data/registration_question/duplicate_username_message" appearance="minimal">
				<label ref="jr:itext('registration_question/duplicate_username_message-label')"/>
			</trigger>
			<secret ref="/data/registration_question/entered_pin_1" appearance="numeric">
				<label ref="jr:itext('registration_question/entered_pin_1-label')"/>
			</secret>
			<secret ref="/data/registration_question/entered_pin_2" appearance="numeric">
				<label ref="jr:itext('registration_question/entered_pin_2-label')"/>
			</secret>
			<select1 ref="/data/registration_question/qualification">
				<label ref="jr:itext('registration_question/qualification-label')"/>
				<item>
					<label ref="jr:itext('registration_question/qualification-ide-label')"/>
					<value>ide</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-ib-label')"/>
					<value>ib</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-ab-label')"/>
					<value>ab</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-accoucheuse-label')"/>
					<value>accoucheuse</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-fe_me-label')"/>
					<value>fe_me</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-ais-label')"/>
					<value>ais</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/qualification-other-label')"/>
					<value>other</value>
				</item>
			</select1>
			<input ref="/data/registration_question/user_phone_number" appearance="numeric">
				<label ref="jr:itext('registration_question/user_phone_number-label')"/>
			</input>
			<select1 ref="/data/registration_question/come_from_another_clinic">
				<label ref="jr:itext('registration_question/come_from_another_clinic-label')"/>
				<item>
					<label ref="jr:itext('registration_question/come_from_another_clinic-yes-label')"/>
					<value>yes</value>
				</item>
				<item>
					<label ref="jr:itext('registration_question/come_from_another_clinic-no-label')"/>
					<value>no</value>
				</item>
			</select1>
		</group>
		<select1 ref="/data/previous_clinic_region">
			<label ref="jr:itext('registration_question/previous_clinic_region-label')"/>
			<itemset nodeset="instance('locations')/regions/region">
				<label ref="name"/>
				<value ref="site_code"/>
