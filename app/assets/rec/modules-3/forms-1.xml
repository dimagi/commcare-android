						<value>user pin matched: <output value="/data/user_pin_matched"/></value>
					</text>
					<text id="debug/debug_admin_pin_matched-label">
						<value>admin pin matched: <output value="/data/admin_pin_matched"/></value>
					</text>
					<text id="debug/debug_security_answers_matched-label">
						<value>debug_security_answers_matched: <output value="/data/security_questions_matched"/></value>
					</text>
					<text id="debug/debug_authentication_successful-label">
						<value>debug_authentication_successful: <output value="/data/authentication_successful"/></value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/change_security_answers_group/new_mother_first_name" nodeset="/data/case/update/mother_first_name" relevant="count(/data/change_security_answers_group/new_mother_first_name) &gt; 0"/><bind calculate="/data/registration_question/entered_pin_1" nodeset="/data/case/update/pin" relevant="count(/data/registration_question/entered_pin_1) &gt; 0"/><bind calculate="/data/change_details_grp/new_qualification" nodeset="/data/case/update/qualification" relevant="count(/data/change_details_grp/new_qualification) &gt; 0"/><bind calculate="/data/change_security_answers_group/new_user_dob" nodeset="/data/case/update/user_dob" relevant="count(/data/change_security_answers_group/new_user_dob) &gt; 0"/><bind calculate="/data/change_details_grp/new_phone_number" nodeset="/data/case/update/user_phone_number" relevant="count(/data/change_details_grp/new_phone_number) &gt; 0"/><setvalue event="xforms-ready" ref="/data/change_details_grp/new_qualification" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/qualification"/><setvalue event="xforms-ready" ref="/data/current_user_pin" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/pin"/><setvalue event="xforms-ready" ref="/data/user_id" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/user_identifier"/><setvalue event="xforms-ready" ref="/data/change_details_grp/new_phone_number" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/user_phone_number"/><setvalue event="xforms-ready" ref="/data/username" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/username"/><setvalue event="xforms-ready" ref="/data/user_dob" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/user_dob"/><setvalue event="xforms-ready" ref="/data/mother_first_name" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/mother_first_name"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<group ref="/data/authentication" appearance="field-list">
			<label ref="jr:itext('authentication-label')"/>
			<select1 ref="/data/authentication/authentication_method">
				<label ref="jr:itext('authentication/authentication_method-label')"/>
				<item>
					<label ref="jr:itext('authentication/authentication_method-using_pin-label')"/>
					<value>using_pin</value>
				</item>
				<item>
					<label ref="jr:itext('authentication/authentication_method-using_security_questions-label')"/>
					<value>using_security_questions</value>
				</item>
				<item>
					<label ref="jr:itext('authentication/authentication_method-using_admin-label')"/>
					<value>using_admin</value>
				</item>
			</select1>
			<secret ref="/data/authentication/authentication_user_pin">
				<label ref="jr:itext('authentication/authentication_user_pin-label')"/>
			</secret>
			<secret ref="/data/authentication/authentication_admin_pin">
				<label ref="jr:itext('authentication/authentication_admin_pin-label')"/>
			</secret>
			<input ref="/data/authentication/authentication_user_dob">
				<label ref="jr:itext('authentication/authentication_user_dob-label')"/>
			</input>
			<input ref="/data/authentication/authentication_user_mother_first_name">
				<label ref="jr:itext('authentication/authentication_user_mother_first_name-label')"/>
			</input>
		</group>
		<group ref="/data/actions_to_do" appearance="field-list">
			<label ref="jr:itext('actions_to_do-label')"/>
			<select ref="/data/actions_to_do/questions_to_edit">
				<label ref="jr:itext('actions_to_do/questions_to_edit-label')"/>
				<item>
					<label ref="jr:itext('actions_to_do/questions_to_edit-qualification-label')"/>
					<value>qualification</value>
				</item>
				<item>
					<label ref="jr:itext('actions_to_do/questions_to_edit-phone_number-label')"/>
					<value>phone_number</value>
				</item>
				<item>
					<label ref="jr:itext('actions_to_do/questions_to_edit-pwd-label')"/>
					<value>pwd</value>
				</item>
				<item>
					<label ref="jr:itext('actions_to_do/questions_to_edit-dob-label')"/>
					<value>dob</value>
				</item>
				<item>
					<label ref="jr:itext('actions_to_do/questions_to_edit-mother_name-label')"/>
					<value>mother_name</value>
				</item>
			</select>
		</group>
		<group ref="/data/change_details_grp" appearance="field-list">
			<label ref="jr:itext('change_details_grp-label')"/>
			<input ref="/data/change_details_grp/new_phone_number" appearance="numeric">
				<label ref="jr:itext('change_details_grp/new_phone_number-label')"/>
			</input>
			<select1 ref="/data/change_details_grp/new_qualification">
				<label ref="jr:itext('change_details_grp/new_qualification-label')"/>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-ide-label')"/>
					<value>ide</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-ib-label')"/>
					<value>ib</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-ab-label')"/>
					<value>ab</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-accoucheuse-label')"/>
					<value>accoucheuse</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-fe_me-label')"/>
					<value>fe_me</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-ais-label')"/>
					<value>ais</value>
				</item>
				<item>
					<label ref="jr:itext('change_details_grp/new_qualification-other-label')"/>
					<value>other</value>
				</item>
			</select1>
		</group>
		<group ref="/data/registration_question" appearance="field-list">
			<label ref="jr:itext('registration_question-label')"/>
			<secret ref="/data/registration_question/entered_pin_1">
				<label ref="jr:itext('registration_question/entered_pin_1-label')"/>
			</secret>
			<secret ref="/data/registration_question/entered_pin_2">
				<label ref="jr:itext('registration_question/entered_pin_2-label')"/>
			</secret>
		</group>
		<group ref="/data/change_security_answers_group" appearance="field-list">
			<label ref="jr:itext('question6-label')"/>
			<input ref="/data/change_security_answers_group/new_user_dob">
				<label ref="jr:itext('change_security_answers_group/new_user_dob-label')"/>
			</input>
			<input ref="/data/change_security_answers_group/new_mother_first_name">
				<label ref="jr:itext('change_security_answers_group/new_mother_first_name-label')"/>
				<hint ref="jr:itext('change_security_answers_group/new_mother_first_name-hint')"/>
			</input>
		</group>
		<group ref="/data/questions_recap" appearance="field-list">
			<label ref="jr:itext('questions_recap-label')"/>
			<trigger ref="/data/questions_recap/recap_qualification" appearance="minimal">
				<label ref="jr:itext('questions_recap/recap_qualification-label')"/>
			</trigger>
			<trigger ref="/data/questions_recap/recap_phone" appearance="minimal">
				<label ref="jr:itext('questions_recap/recap_phone-label')"/>
			</trigger>
			<trigger ref="/data/questions_recap/recap_pin" appearance="minimal">
				<label ref="jr:itext('questions_recap/recap_pin-label')"/>
			</trigger>
			<trigger ref="/data/questions_recap/recap_dob" appearance="minimal">
				<label ref="jr:itext('questions_recap/recap_dob-label')"/>
			</trigger>
			<trigger ref="/data/questions_recap/recap_mother_name" appearance="minimal">
				<label ref="jr:itext('questions_recap/recap_mother_name-label')"/>
			</trigger>
			<trigger ref="/data/questions_recap/recap_confirm">
				<label ref="jr:itext('questions_recap/recap_confirm-label')"/>
			</trigger>
		</group>
		<group ref="/data/debug">
			<label ref="jr:itext('debug-label')"/>
			<trigger ref="/data/debug/debug_admin_pin" appearance="minimal">
				<label ref="jr:itext('debug/debug_admin_pin-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_user_pin" appearance="minimal">
				<label ref="jr:itext('debug/debug_user_pin-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_dob" appearance="minimal">
				<label ref="jr:itext('debug/debug_dob-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_mother_first_name" appearance="minimal">
				<label ref="jr:itext('debug/debug_mother_first_name-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_user_pin_matched" appearance="minimal">
				<label ref="jr:itext('debug/debug_user_pin_matched-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_admin_pin_matched" appearance="minimal">
				<label ref="jr:itext('debug/debug_admin_pin_matched-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_security_answers_matched" appearance="minimal">
				<label ref="jr:itext('debug/debug_security_answers_matched-label')"/>
			</trigger>
			<trigger ref="/data/debug/debug_authentication_successful" appearance="minimal">
				<label ref="jr:itext('debug/debug_authentication_successful-label')"/>
			</trigger>
		</group>
	</h:body>
</h:html>