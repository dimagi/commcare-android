<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Register Face</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/CE5399FA-0044-4AD3-B933-14A02E818EC6" uiVersion="1" version="22" name="Register Face">
					<name/>
					<intentCallout/>
					<image1/>
					<image2/>
					<image3/>
					<recognize_action>register</recognize_action>
					<my_case_id/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/>
                    <case_type>scanned</case_type></create>
                    <update><my_case_id/></update>
                    <attachment><image1 from="local" src=""/><image2 from="local" src=""/><image3 from="local" src=""/></attachment>
                </case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance>
            <instance id="commcaresession" src="jr://instance/session"/>
            <setvalue event="xforms-ready" ref="/data/my_case_id" value="instance('commcaresession')/session/data/case_id_new_scanned_0"/>
			<bind nodeset="/data/name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/intentCallout" type="intent"/>
			<bind nodeset="/data/image1" type="binary"/>
			<bind nodeset="/data/image2" type="binary"/>
			<bind nodeset="/data/image3" type="binary"/>
			<bind nodeset="/data/recognize_action"/>
			<bind nodeset="/data/my_case_id"/>
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>Name</value>
					</text>
					<text id="intentCallout-label">
						<value>Intent Callout</value>
					</text>
					<text id="image1-label">
						<value>image1</value>
					</text>
					<text id="image2-label">
						<value>image2</value>
					</text>
					<text id="image3-label">
						<value>image3</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><setvalue event="xforms-ready" ref="/data/case/@case_id" value="instance('commcaresession')/session/data/case_id_new_scanned_0"/><bind calculate="/data/name" nodeset="/data/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/case/create/owner_id"/><bind calculate="/data/my_case_id" nodeset="/data/case/update/my_case_id" relevant="count(/data/my_case_id) &gt; 0"/><bind nodeset="/data/case/attachment/image1" relevant="count(/data/image1) = 1"/><bind calculate="/data/image1" nodeset="/data/case/attachment/image1/@src"/><bind nodeset="/data/case/attachment/image2" relevant="count(/data/image2) = 1"/><bind calculate="/data/image2" nodeset="/data/case/attachment/image2/@src"/><bind nodeset="/data/case/attachment/image3" relevant="count(/data/image3) = 1"/><bind calculate="/data/image3" nodeset="/data/case/attachment/image3/@src"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
		<odkx:intent xmlns:odkx="http://opendatakit.org/xforms" id="intentCallout" class="org.opencv.javacv.facerecognition.LAUNCH">
			<extra key="recognize_action" ref="/data/recognize_action"/>
			<extra key="case_id" ref="/data/my_case_id"/>
            <response key="image_1" ref="/data/image1"/>
            <response key="image_2" ref="/data/image2"/>
            <response key="image_3" ref="/data/image3"/>
		</odkx:intent>
	</h:head>
	<h:body>
		<input ref="/data/name">
			<label ref="jr:itext('name-label')"/>
		</input>
		<input ref="/data/intentCallout" appearance="intent:intentCallout">
			<label ref="jr:itext('intentCallout-label')"/>
		</input>
		<upload mediatype="image/*" ref="/data/image1">
			<label ref="jr:itext('image1-label')"/>
		</upload>
		<upload mediatype="image/*" ref="/data/image2">
			<label ref="jr:itext('image2-label')"/>
		</upload>
		<upload mediatype="image/*" ref="/data/image3">
			<label ref="jr:itext('image3-label')"/>
		</upload>
	</h:body>
</h:html>