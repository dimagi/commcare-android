<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>Update Case</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/FFA854BD-61DE-4CE2-89BC-5176D4EB9148" uiVersion="1" version="238" name="Update Case">
					<eye_colour/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><eye_colour/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/eye_colour"/>
			<itext>
				<translation lang="en">
					<text id="eye_colour-label">
						<value>Select an eye colour</value>
					</text>
					<text id="eye_colour-blue-label">
						<value>Blue</value>
					</text>
					<text id="eye_colour-brown-label">
						<value>Brown</value>
					</text>
					<text id="eye_colour-black-label">
						<value>Black</value>
					</text>
					<text id="eye_colour-green-label">
						<value>Green</value>
					</text>
				</translation>
				<translation lang="hin" default="">
					<text id="eye_colour-label">
						<value>HIN: Select an eye colour</value>
					</text>
					<text id="eye_colour-blue-label">
						<value>HIN: Blue</value>
					</text>
					<text id="eye_colour-brown-label">
						<value>HIN: Brown</value>
					</text>
					<text id="eye_colour-black-label">
						<value>HIN: Black</value>
					</text>
					<text id="eye_colour-green-label">
						<value>HIN: Green</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/eye_colour" nodeset="/data/case/update/eye_colour" relevant="count(/data/eye_colour) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/eye_colour">
			<label ref="jr:itext('eye_colour-label')"/>
			<item>
				<label ref="jr:itext('eye_colour-blue-label')"/>
				<value>blue</value>
			</item>
			<item>
				<label ref="jr:itext('eye_colour-brown-label')"/>
				<value>brown</value>
			</item>
			<item>
				<label ref="jr:itext('eye_colour-black-label')"/>
				<value>black</value>
			</item>
			<item>
				<label ref="jr:itext('eye_colour-green-label')"/>
				<value>green</value>
			</item>
		</select1>
	</h:body>
</h:html>