<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>11. Sputum1 Results</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/BAC13CD6-EF24-4F2B-9B3F-AD4ADA50B62E" uiVersion="1" version="1972" name="11. Sputum1 Results">
					<sputum1rcvd/>
					<spt1dlvrd/>
					<sput1appearance/>
					<smear1performed/>
					<sput1rsltspos/>
					<xptsptm1/>
					<Xpert1Rslts/>
					<xpmtbsemiquant/>
					<xpertrifres/>
					<comments/>
					<confirmtb/>
					<pttype/>
					<allocation/>
					<screenid/>
					<age/>
					<gender/>
					<ptid/>
					<tb_status/>
					<tb_risk/>
					<hiv_status/>
					<send_SMS/>
					<target_clinic_visit_date/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><Xpert1Rslts/><hiv_status/><send_SMS/><target_clinic_visit_date/><tb_risk/><tb_status/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/sputum1rcvd" required="true()"/>
			<bind nodeset="/data/spt1dlvrd" type="xsd:dateTime" relevant="/data/sputum1rcvd = 'yes'" required="true()"/>
			<bind nodeset="/data/sput1appearance" relevant="/data/sputum1rcvd = 'yes'" required="true()"/>
			<bind nodeset="/data/smear1performed" relevant=" /data/sputum1rcvd = 'yes'"/>
			<bind nodeset="/data/sput1rsltspos" relevant="/data/smear1performed = 'yes'" required="true()"/>
			<bind nodeset="/data/xptsptm1" relevant="/data/sputum1rcvd = 'yes'" required="true()"/>
			<bind nodeset="/data/Xpert1Rslts" relevant="/data/xptsptm1 = 'yes'" required="true()"/>
			<bind nodeset="/data/xpmtbsemiquant" relevant="/data/xptsptm1 = 'yes' and /data/Xpert1Rslts = 'positive'" required="true()"/>
			<bind nodeset="/data/xpertrifres" relevant="/data/Xpert1Rslts = 'positive'" required="true()"/>
			<bind nodeset="/data/comments" type="xsd:string" required="false()"/>
			<bind nodeset="/data/confirmtb" type="binary" relevant=" /data/Xpert1Rslts = 'positive'"/>
			<bind nodeset="/data/pttype"/>
			<bind nodeset="/data/allocation"/>
			<bind nodeset="/data/screenid"/>
			<bind nodeset="/data/age"/>
			<bind nodeset="/data/gender"/>
			<bind nodeset="/data/ptid"/>
			<bind nodeset="/data/tb_status" calculate="if(/data/sput1rsltspos = &quot;3&quot; or /data/sput1rsltspos = &quot;2&quot; or /data/sput1rsltspos = &quot;1&quot;, &quot;confirmed&quot;, if(/data/sput1rsltspos =&quot;neg&quot;, &quot;negative&quot;, if(/data/Xpert1Rslts = &quot;confirmed&quot;, &quot;confirmed&quot;,if (/data/Xpert1Rslts =&quot;negative&quot;, &quot;negative&quot;, &quot;not_testable&quot;))))"/>
			<bind nodeset="/data/tb_risk" calculate="if(/data/hiv_status = &quot;likely&quot; or /data/age &lt;5, &quot;likely&quot;,if(/data/tb_status = &quot;confirmed&quot;, &quot;confirmed&quot;,&quot;unlikely&quot;))"/>
			<bind nodeset="/data/hiv_status"/>
			<bind nodeset="/data/send_SMS" calculate="if(/data/allocation = &quot;B&quot;, &quot;control&quot;, if(/data/tb_status = &quot;confirmed&quot;, &quot;confirmed&quot;, if (/data/tb_status = &quot;negative&quot; or /data/tb_risk = &quot;unlikely&quot;, &quot;unlikely&quot;, if(/data/tb_risk = &quot;likely&quot;, &quot;likely&quot;,&quot;not_testable&quot;))))"/>
			<bind nodeset="/data/target_clinic_visit_date" calculate="today() +5"/>
			<itext>
				<translation lang="en" default="">
					<text id="sputum1rcvd-label">
						<value>Was sputum 1  received in laboratory?</value>
					</text>
					<text id="sputum1rcvd-yes-label">
						<value>Yes </value>
					</text>
					<text id="spt1dlvrd-label">
						<value>Date and time sputum 1 was delivered to Laboratory </value>
					</text>
					<text id="spt1dlvrd-hint">
						<value>(24 hour clock)</value>
					</text>
					<text id="sput1appearance-label">
						<value>Appearance of sputum 1</value>
					</text>
					<text id="sput1appearance-blood_stained-label">
						<value>Blood-stained</value>
					</text>
					<text id="sput1appearance-muco_purulent-label">
						<value>Mucopurulent</value>
					</text>
					<text id="sput1appearance-mucoid-label">
						<value>Mucoid</value>
					</text>
					<text id="sput1appearance-salivary-label">
						<value>Salivary</value>
					</text>
					<text id="sput1appearance-notrecorded-label">
						<value>Not recorded </value>
					</text>
					<text id="smear1performed-label">
						<value>Was smear microscopy performed on sputum 1?</value>
					</text>
					<text id="smear1performed-yes-label">
						<value>Yes</value>
					</text>
					<text id="smear1performed-no-label">
						<value>No</value>
					</text>
					<text id="sput1rsltspos-label">
						<value>Smear microscopy results on sputum 1</value>
					</text>
					<text id="sput1rsltspos-3-label">
						<value>Positive 3+</value>
					</text>
					<text id="sput1rsltspos-2-label">
						<value>Positive 2+</value>
					</text>
					<text id="sput1rsltspos-1-label">
						<value>Positve 1+</value>
					</text>
					<text id="sput1rsltspos-scanty-label">
						<value>Scanty</value>
					</text>
					<text id="xptsptm1-label">
						<value>Was Xpert performed on sputum 1?</value>
					</text>
					<text id="Xpert1Rslts-label">
						<value> Xpert results on sputum 1</value>
					</text>
					<text id="Xpert1Rslts-positive-label">
						<value>Positive</value>
					</text>
					<text id="Xpert1Rslts-negative-label">
						<value>Negative</value>
					</text>
					<text id="Xpert1Rslts-invalid-label">
						<value>Invalid</value>
					</text>
					<text id="Xpert1Rslts-error-label">
						<value>Error</value>
					</text>
					<text id="Xpert1Rslts-xpmissingrst-label">
						<value>Missing result</value>
					</text>
					<text id="xpmtbsemiquant-label">
						<value>Xpert MTB semi-quantitative result</value>
					</text>
					<text id="xpmtbsemiquant-verylow-label">
						<value>Very Low</value>
					</text>
					<text id="xpmtbsemiquant-low-label">
						<value>Low</value>
					</text>
					<text id="xpmtbsemiquant-meduim-label">
						<value>Medium</value>
					</text>
					<text id="xpmtbsemiquant-high-label">
						<value>High</value>
					</text>
					<text id="xpertrifres-label">
						<value>Xpert RIF result</value>
					</text>
					<text id="xpertrifres-notdetected-label">
						<value>Rif Resistance not detected</value>
					</text>
					<text id="xpertrifres-detected-label">
						<value>Rif Resistance detected</value>
					</text>
					<text id="xpertrifres-interminate-label">
						<value>Rif Resistance indeterminate</value>
					</text>
					<text id="comments-label">
						<value>Free text comments to explain unusual situations encountered during sputum evaluation process.</value>
					</text>
					<text id="confirmtb-label">
						<value>Please confirm cover up the names of the other people in the register and take a picture of the patient's diagnosis.</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/Xpert1Rslts" nodeset="/data/case/update/Xpert1Rslts" relevant="count(/data/Xpert1Rslts) &gt; 0"/><bind calculate="/data/hiv_status" nodeset="/data/case/update/hiv_status" relevant="count(/data/hiv_status) &gt; 0"/><bind calculate="/data/send_SMS" nodeset="/data/case/update/send_SMS" relevant="count(/data/send_SMS) &gt; 0"/><bind calculate="/data/target_clinic_visit_date" nodeset="/data/case/update/target_clinic_visit_date" relevant="count(/data/target_clinic_visit_date) &gt; 0"/><bind calculate="/data/tb_risk" nodeset="/data/case/update/tb_risk" relevant="count(/data/tb_risk) &gt; 0"/><bind calculate="/data/tb_status" nodeset="/data/case/update/tb_status" relevant="count(/data/tb_status) &gt; 0"/><setvalue event="xforms-ready" ref="/data/gender" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/ptgender"/><setvalue event="xforms-ready" ref="/data/screenid" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/screenid"/><setvalue event="xforms-ready" ref="/data/send_SMS" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/send_SMS"/><setvalue event="xforms-ready" ref="/data/tb_status" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/tb_status"/><setvalue event="xforms-ready" ref="/data/pttype" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/pttype"/><setvalue event="xforms-ready" ref="/data/ptid" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/ptid"/><setvalue event="xforms-ready" ref="/data/tb_risk" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/tb_risk"/><setvalue event="xforms-ready" ref="/data/hiv_status" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/hiv_status"/><setvalue event="xforms-ready" ref="/data/allocation" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/allocation"/><setvalue event="xforms-ready" ref="/data/age" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/ptage"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/sputum1rcvd">
			<label ref="jr:itext('sputum1rcvd-label')"/>
			<item>
				<label ref="jr:itext('sputum1rcvd-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('smear1performed-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<input ref="/data/spt1dlvrd">
			<label ref="jr:itext('spt1dlvrd-label')"/>
			<hint ref="jr:itext('spt1dlvrd-hint')"/>
		</input>
		<select1 ref="/data/sput1appearance">
			<label ref="jr:itext('sput1appearance-label')"/>
			<item>
				<label ref="jr:itext('sput1appearance-blood_stained-label')"/>
				<value>blood_stained</value>
			</item>
			<item>
				<label ref="jr:itext('sput1appearance-muco_purulent-label')"/>
				<value>muco_purulent</value>
			</item>
			<item>
				<label ref="jr:itext('sput1appearance-mucoid-label')"/>
				<value>mucoid</value>
			</item>
			<item>
				<label ref="jr:itext('sput1appearance-salivary-label')"/>
				<value>salivary</value>
			</item>
			<item>
				<label ref="jr:itext('sput1appearance-notrecorded-label')"/>
				<value>notrecorded</value>
			</item>
		</select1>
		<select1 ref="/data/smear1performed">
			<label ref="jr:itext('smear1performed-label')"/>
			<item>
				<label ref="jr:itext('smear1performed-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('smear1performed-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<select1 ref="/data/sput1rsltspos">
			<label ref="jr:itext('sput1rsltspos-label')"/>
			<item>
				<label ref="jr:itext('sput1rsltspos-3-label')"/>
				<value>3</value>
			</item>
			<item>
				<label ref="jr:itext('sput1rsltspos-2-label')"/>
				<value>2</value>
			</item>
			<item>
				<label ref="jr:itext('sput1rsltspos-1-label')"/>
				<value>1</value>
			</item>
			<item>
				<label ref="jr:itext('sput1rsltspos-scanty-label')"/>
				<value>scanty</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-negative-label')"/>
				<value>neg</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-xpmissingrst-label')"/>
				<value>missing</value>
			</item>
		</select1>
		<select1 ref="/data/xptsptm1">
			<label ref="jr:itext('xptsptm1-label')"/>
			<item>
				<label ref="jr:itext('smear1performed-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('smear1performed-no-label')"/>
				<value>no</value>
			</item>
		</select1>
		<select1 ref="/data/Xpert1Rslts">
			<label ref="jr:itext('Xpert1Rslts-label')"/>
			<item>
				<label ref="jr:itext('Xpert1Rslts-positive-label')"/>
				<value>positive</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-negative-label')"/>
				<value>negative</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-invalid-label')"/>
				<value>invalid</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-error-label')"/>
				<value>error</value>
			</item>
			<item>
				<label ref="jr:itext('Xpert1Rslts-xpmissingrst-label')"/>
				<value>xpmissingrst</value>
			</item>
		</select1>
		<select1 ref="/data/xpmtbsemiquant">
			<label ref="jr:itext('xpmtbsemiquant-label')"/>
			<item>
				<label ref="jr:itext('xpmtbsemiquant-verylow-label')"/>
				<value>verylow</value>
			</item>
			<item>
				<label ref="jr:itext('xpmtbsemiquant-low-label')"/>
				<value>low</value>
			</item>
			<item>
				<label ref="jr:itext('xpmtbsemiquant-meduim-label')"/>
				<value>meduim</value>
			</item>
			<item>
				<label ref="jr:itext('xpmtbsemiquant-high-label')"/>
				<value>high</value>
			</item>
		</select1>
		<select1 ref="/data/xpertrifres">
			<label ref="jr:itext('xpertrifres-label')"/>
			<item>
				<label ref="jr:itext('xpertrifres-notdetected-label')"/>
				<value>notdetected</value>
			</item>
			<item>
				<label ref="jr:itext('xpertrifres-detected-label')"/>
				<value>detected</value>
			</item>
			<item>
				<label ref="jr:itext('xpertrifres-interminate-label')"/>
				<value>interminate</value>
			</item>
		</select1>
		<input ref="/data/comments">
			<label ref="jr:itext('comments-label')"/>
		</input>
		<upload mediatype="image/*" ref="/data/confirmtb">
			<label ref="jr:itext('confirmtb-label')"/>
		</upload>
	</h:body>
</h:html>