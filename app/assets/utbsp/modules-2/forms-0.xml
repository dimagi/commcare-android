				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><contact_phone_number/><healthcentre/><hiv_status/><phone_no/><ptage/><ptgender/><pttype/><send_SMS/><tb_risk/><tb_status/><verification_code/><verification_code_trigger/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/notEligibleLabel" constraint="'1'='0'" relevant="/data/eligible = 'no'" required="true()"/>
			<bind nodeset="/data/birthdate" type="xsd:date" constraint=".&lt;=today()" jr:constraintMsg="jr:itext('birthdate-constraintMsg')" required="true()"/>
			<bind nodeset="/data/age_year_display" relevant="/data/age&gt;=2" required="true()"/>
			<bind nodeset="/data/age_month_display" relevant="/data/age&lt;=2"/>
			<bind nodeset="/data/gender" required="true()"/>
			<bind nodeset="/data/relationship_to_contact" required="true()"/>
			<bind nodeset="/data/sms_registration"/>
			<bind nodeset="/data/sms_registration/phone_no" type="xsd:string" constraint="string-length(.) =9" jr:constraintMsg="jr:itext('phone_no-constraintMsg')" required="true()"/>
			<bind nodeset="/data/sms_registration/phone_no_for_SMS" calculate="concat('256', substr(/data/sms_registration/phone_no,1, 10))"/>
			<bind nodeset="/data/sms_registration/wiling_to__receive_msgs" required="true()"/>
			<bind nodeset="/data/smspreflang" required="true()"/>
			<bind nodeset="/data/preferedname" type="xsd:string" required="true()"/>
			<bind nodeset="/data/clinicalhistory"/>
			<bind nodeset="/data/clinicalhistory/hiv_status" required="true()"/>
			<bind nodeset="/data/clinicalhistory/hivposenrcln" relevant="/data/clinicalhistory/hiv_status = 'positive'" required="true()"/>
			<bind nodeset="/data/clinicalhistory/tested_last_three_months" relevant="/data/clinicalhistory/hiv_status = 'negative'" required="true()"/>
			<bind nodeset="/data/clinicalhistory/agree_to_test" relevant="/data/clinicalhistory/hiv_status = 'unknown' and /data/clinicalhistory/tested_last_three_months = 'no'" required="true()"/>
			<bind nodeset="/data/clinicalhistory/taking_arvs" relevant="/data/clinicalhistory/hiv_status = 'positive'" required="true()"/>
			<bind nodeset="/data/tbscreening"/>
			<bind nodeset="/data/tbscreening/have_cough" required="true()"/>
			<bind nodeset="/data/tbscreening/cough2wks" relevant="/data/tbscreening/have_cough = 'yes'" required="true()"/>
			<bind nodeset="/data/tbscreening/hadfeverlstmo" required="true()"/>
			<bind nodeset="/data/tbscreening/weight_loss" required="true()"/>
			<bind nodeset="/data/tbscreening/ntsweatsmo" required="true()"/>
			<bind nodeset="/data/tbscreening/been_evaluated" relevant="/data/tbscreening/have_cough = 'yes' or /data/tbscreening/cough2wks = 'yes' or /data/tbscreening/hadfeverlstmo = 'yes' or /data/tbscreening/weight_loss = 'yes'" required="true()"/>
			<bind nodeset="/data/follow_up_information"/>
			<bind nodeset="/data/follow_up_information/planned_for_clinic_evaluation" relevant="/data/tbscreening/have_cough = 'yes' or /data/tbscreening/cough2wks = 'yes' or /data/tbscreening/hadfeverlstmo = 'yes' or /data/tbscreening/weight_loss = 'yes'" required="true()"/>
			<bind nodeset="/data/follow_up_information/kcca_health_facility" relevant="/data/follow_up_information/planned_for_clinic_evaluation = 'yes'" required="true()"/>
			<bind nodeset="/data/follow_up_information/specifichc" relevant="/data/follow_up_information/kcca_health_facility = 'other'"/>
			<bind nodeset="/data/follow_up_information/otherfacility" type="xsd:string" relevant="/data/follow_up_information/specifichc = 'otherspecify'" required="true()"/>
			<bind nodeset="/data/hhhead" relevant="/data/age&gt;18" required="true()"/>
			<bind nodeset="/data/householdse" relevant="/data/hhhead = 'yes'"/>
			<bind nodeset="/data/householdse/ptoccupation" required="true()"/>
			<bind nodeset="/data/householdse/spoccupation" type="xsd:string" relevant="/data/householdse/ptoccupation = 'other'" required="true()"/>
			<bind nodeset="/data/householdse/schoollevel" required="true()"/>
			<bind nodeset="/data/householdse/ablread" required="true()"/>
			<bind nodeset="/data/householdse/hhIncome" type="xsd:int" constraint=".&gt;=0" jr:constraintMsg="jr:itext('householdse/hhIncome-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/surehhIncome" relevant="/data/householdse/hhIncome &gt; 1000" required="true()"/>
			<bind nodeset="/data/householdse/hhExpenditure" type="xsd:string" constraint=".&gt;=0" jr:constraintMsg="jr:itext('householdse/hhExpenditure-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/hhExpendituresure" relevant="/data/householdse/hhExpenditure &gt; 1000" required="true()"/>
			<bind nodeset="/data/householdse/transmode" required="true()"/>
			<bind nodeset="/data/householdse/spectransmode" type="xsd:string" relevant="/data/householdse/transmode = 'other'" required="true()"/>
			<bind nodeset="/data/householdse/timespentintrans" type="xsd:int" constraint=".&gt;=0 and .&lt;=168" jr:constraintMsg="jr:itext('householdse/timespentintrans-constraintMsg')"/>
			<bind nodeset="/data/householdse/moneyspenttrans" type="xsd:int" constraint=".&gt;=0 " jr:constraintMsg="jr:itext('householdse/moneyspenttrans-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/surespenttrans" relevant="/data/householdse/moneyspenttrans &gt; 100" required="true()"/>
			<bind nodeset="/data/householdse/indexYrsNeighborhood" type="xsd:int" constraint=". &gt;= 0 and . &lt;= 120" jr:constraintMsg="jr:itext('indexYrsNeighborhood-constraintMsg')" required="true()"/>
			<bind nodeset="/data/question226"/>
			<bind nodeset="/data/age" calculate="int((today() - date(/data/birthdate)) div 365.25)"/>
			<bind nodeset="/data/age_in_months" calculate="if(/data/age &lt;= 2, int((today() - date(/data/birthdate)) div 30.4), '')"/>
			<bind nodeset="/data/index_patient_id"/>
			<bind nodeset="/data/cIntvwDate"/>
			<bind nodeset="/data/pttype" calculate="'contact'"/>
			<bind nodeset="/data/screenid"/>
			<bind nodeset="/data/ctscreenid"/>
			<bind nodeset="/data/eligible"/>
			<bind nodeset="/data/allocation"/>
			<bind nodeset="/data/allocation_id"/>
			<bind nodeset="/data/verification_code" relevant="/data/sms_registration/wiling_to__receive_msgs = 'yes'" calculate="uuid(4)"/>
			<bind nodeset="/data/contact_phone_number" relevant="/data/sms_registration/wiling_to__receive_msgs = 'yes'" calculate="concat('256', /data/sms_registration/phone_no)"/>
			<bind nodeset="/data/verification_code_trigger" calculate="'A'"/>
			<bind nodeset="/data/tb_status"/>
			<bind nodeset="/data/tb_risk" calculate="if(/data/clinicalhistory/hiv_status = 'positive' or /data/age &lt;= 5 or /data/tbscreening/have_cough = 'yes' or /data/tbscreening/cough2wks = 'yes' or /data/tbscreening/hadfeverlstmo='yes' or  /data/tbscreening/weight_loss = 'yes' or /data/tbscreening/ntsweatsmo = 'yes'  , 'likely', 'unlikely')"/>
			<bind nodeset="/data/target_clinic_visit_date"/>
			<bind nodeset="/data/send_SMS" calculate="if(/data/allocation = &quot;B&quot;, &quot;control&quot;, if(/data/tb_status = &quot;positive&quot;, &quot;confirmed&quot;, if (/data/tb_status = &quot;negative&quot; or /data/tb_risk = &quot;unlikely&quot;, &quot;unlikely&quot;, if(/data/tb_risk = &quot;likely&quot;, &quot;likely&quot;,&quot;not_testable&quot;))))"/>
