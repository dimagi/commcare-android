<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>13 Under Five Contact Clinic Evaluation</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/6ed6b9fb16af269db691f324bf77f0ea1bc82ad" uiVersion="1" version="1781" name="13 Under Five Contact Clinic Evaluation">
					<visithivcln/>
					<fstvisitdt/>
					<rxactivetb/>
					<additionaleval/>
					<rsltscnfm/>
					<dtsttbrx/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""/><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data></instance><instance id="commcaresession" src="jr://instance/session"/><instance id="casedb" src="jr://instance/casedb"/>
			<bind nodeset="/data/visithivcln" required="true()"/>
			<bind nodeset="/data/fstvisitdt" type="xsd:date" relevant="/data/visithivcln = 'yes'" required="true()"/>
			<bind nodeset="/data/rxactivetb" required="true()"/>
			<bind nodeset="/data/additionaleval" relevant=" /data/rxactivetb = 'undecided'" required="true()"/>
			<bind nodeset="/data/rsltscnfm" relevant="/data/rxactivetb = 'yes'" required="true()"/>
			<bind nodeset="/data/dtsttbrx" type="xsd:date" relevant="/data/rxactivetb = 'yes'" required="true()"/>
			<itext>
				<translation lang="en" default="">
					<text id="visithivcln-label">
						<value>Did the patient visit the HIV clinic?</value>
					</text>
					<text id="visithivcln-notapplicable-label">
						<value>Not Applicable</value>
					</text>
					<text id="fstvisitdt-label">
						<value>What date did the patient first visit the HIV clinic?</value>
					</text>
					<text id="rxactivetb-label">
						<value>Is the contact going to be treated for active TB?</value>
					</text>
					<text id="rxactivetb-yes-label">
						<value>Yes</value>
					</text>
					<text id="rxactivetb-no-label">
						<value>No</value>
					</text>
					<text id="rxactivetb-undecided-label">
						<value>Undecided - further testing planned</value>
					</text>
					<text id="additionaleval-label">
						<value>What additional evaluation is planned ( Choose all that apply) ?</value>
					</text>
					<text id="additionaleval-reftomulago-label">
						<value>Referral to Mulago</value>
					</text>
					<text id="additionaleval-refcxr-label">
						<value>Referral for CXR</value>
					</text>
					<text id="additionaleval-refoutsideprov-label">
						<value>Referral to Outside provider</value>
					</text>
					<text id="rsltscnfm-label">
						<value>What is the basis for TB diagnosis and/or treatment?</value>
					</text>
					<text id="rsltscnfm-smear_positive-label">
						<value>Smear-positive</value>
					</text>
					<text id="rsltscnfm-xpert-label">
						<value>Xpert-positive</value>
					</text>
					<text id="rsltscnfm-culture-label">
						<value>Culture-positive</value>
					</text>
					<text id="rsltscnfm-xraytb-label">
						<value>Chest x-ray findings of TB</value>
					</text>
					<text id="rsltscnfm-no_microbiologicorchestxray-label">
						<value>Clinician decision without chest x-ray or confirmed microbiologic result</value>
					</text>
					<text id="dtsttbrx-label">
						<value>What day is the patient starting TB treatment?</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><setvalue event="xforms-ready" ref="/data/dtsttbrx" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/dtsttbrx"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/visithivcln">
			<label ref="jr:itext('visithivcln-label')"/>
			<item>
				<label ref="jr:itext('rxactivetb-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('rxactivetb-no-label')"/>
				<value>no</value>
			</item>
			<item>
				<label ref="jr:itext('visithivcln-notapplicable-label')"/>
				<value>notapplicable</value>
			</item>
		</select1>
		<input ref="/data/fstvisitdt">
			<label ref="jr:itext('fstvisitdt-label')"/>
		</input>
		<select1 ref="/data/rxactivetb">
			<label ref="jr:itext('rxactivetb-label')"/>
			<item>
				<label ref="jr:itext('rxactivetb-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('rxactivetb-no-label')"/>
				<value>no</value>
			</item>
			<item>
				<label ref="jr:itext('rxactivetb-undecided-label')"/>
				<value>undecided</value>
			</item>
		</select1>
		<select ref="/data/additionaleval">
			<label ref="jr:itext('additionaleval-label')"/>
			<item>
				<label ref="jr:itext('additionaleval-reftomulago-label')"/>
				<value>reftomulago</value>
			</item>
			<item>
				<label ref="jr:itext('additionaleval-refcxr-label')"/>
				<value>refcxr</value>
			</item>
			<item>
				<label ref="jr:itext('additionaleval-refoutsideprov-label')"/>
				<value>refoutsideprov</value>
			</item>
		</select>
		<select1 ref="/data/rsltscnfm">
			<label ref="jr:itext('rsltscnfm-label')"/>
			<item>
				<label ref="jr:itext('rsltscnfm-smear_positive-label')"/>
				<value>smear_positive</value>
			</item>
			<item>
				<label ref="jr:itext('rsltscnfm-xpert-label')"/>
				<value>xpert</value>
			</item>
			<item>
				<label ref="jr:itext('rsltscnfm-culture-label')"/>
				<value>culture</value>
			</item>
			<item>
				<label ref="jr:itext('rsltscnfm-xraytb-label')"/>
				<value>xraytb</value>
			</item>
			<item>
				<label ref="jr:itext('rsltscnfm-no_microbiologicorchestxray-label')"/>
				<value>no_microbiologicorchestxray</value>
			</item>
		</select1>
		<input ref="/data/dtsttbrx">
			<label ref="jr:itext('dtsttbrx-label')"/>
		</input>
	</h:body>
</h:html>