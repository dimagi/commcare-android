				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""/><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance>
			<instance id="casedb" src="jr://instance/casedb"/>
			<instance id="commcaresession" src="jr://instance/session"/>
			<bind nodeset="/data/ctverbagmt"/>
			<bind nodeset="/data/cMetCriteria"/>
			<bind nodeset="/data/cable" relevant="/data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes' "/>
			<bind nodeset="/data/cAlreadyOnTx" relevant=" /data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes'"/>
			<bind nodeset="/data/cHvPhone" relevant=" /data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes'"/>
			<bind nodeset="/data/cWillSMS" relevant=" /data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes'"/>
			<bind nodeset="/data/cYesEngLug" relevant=" /data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes'"/>
			<bind nodeset="/data/contactchildunder5"/>
			<bind nodeset="/data/cwilling" relevant=" /data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes'"/>
			<bind nodeset="/data/cEligible" relevant="/data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes' and /data/cYesEngLug = 'yes' and /data/cWillSMS = 'yes' and /data/cHvPhone = 'yes' and /data/cable = 'yes' and /data/cwilling = 'yes' and /data/cAlreadyOnTx = 'no'" required="true()"/>
			<bind nodeset="/data/c_not_Eligible" relevant="not(/data/cable = 'yes' and /data/cHvPhone = 'yes' and /data/cWillSMS = 'yes' and /data/cYesEngLug = 'yes' and /data/ctverbagmt = 'yes' and /data/cMetCriteria = 'yes' and /data/cwilling = 'yes'  and /data/cAlreadyOnTx = 'no')"/>
			<bind nodeset="/data/cpltconsfm" relevant="/data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes' and /data/cYesEngLug = 'no' and /data/cWillSMS = 'no' and /data/cHvPhone = 'no' and /data/cable = 'no' and /data/cwilling = 'no'" required="true()"/>
