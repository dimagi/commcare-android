			<bind nodeset="/data/contact_case"/>
			<bind nodeset="/data/contact_case/case"/><bind calculate="/data/pttype" nodeset="/data/contact_case/case/update/pttype"/><setvalue event="xforms-ready" ref="/data/allocation" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/allocation"/><setvalue event="xforms-ready" ref="/data/allocation_id" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/allocation_id"/><bind calculate="/data/meta/timeEnd" nodeset="/data/contact_case/case/@date_modified" type="xsd:dateTime"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><bind nodeset="/data/contact_case/case/update"/><bind calculate="/data/meta/userID" nodeset="/data/contact_case/case/@user_id"/><bind calculate="concat(/data/ctname/ctgivenname,' ',/data/ctname/ctsurname)" nodeset="/data/contact_case/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/contact_case/case/create/owner_id"/><bind calculate="/data/ctscreenid" nodeset="/data/contact_case/case/update/ctscreenid" relevant="count(/data/ctscreenid) &gt; 0"/><setvalue event="xforms-ready" ref="/data/idxptid_2" value="instance('commcaresession')/session/data/case_id"/><setvalue event="xforms-ready" ref="/data/idxptid" value="instance('casedb')/casedb/case[@case_id=instance('commcaresession')/session/data/case_id]/ptid"/><bind nodeset="/data/contact_case/case/update/eligible" calculate="if(/data/cMetCriteria = 'yes' and /data/ctverbagmt = 'yes' and  /data/cYesEngLug = 'yes' and /data/cWillSMS = 'yes' and /data/cHvPhone = 'yes' and /data/cable = 'yes'  and /data/cwilling = 'yes','yes','no')"/><bind nodeset="/data/contact_case/case/update/right_index" relevant="count(/data/right_index) &gt; 0" calculate="/data/right_index"/><bind nodeset="/data/contact_case/case/update/right_thumb" relevant="count(/data/right_thumb) &gt; 0" calculate="/data/right_thumb"/><bind nodeset="/data/contact_case/case/update/left_index" relevant="count(/data/left_index) &gt; 0" calculate="/data/left_index"/><bind nodeset="/data/contact_case/case/update/left_thumb" relevant="count(/data/left_thumb) &gt; 0" calculate="/data/left_thumb"/><bind nodeset="/data/contact_case/case/update/allocation_id" calculate="/data/allocation_id"/><bind nodeset="/data/contact_case/case/update/allocation" calculate="/data/allocation"/><bind nodeset="/data/contact_case/case/update/indxptid" calculate="/data/idxptid_2"/><setvalue event="xforms-ready" ref="/data/contact_case/case/@case_id" value="uuid()"/><bind nodeset="/data/contact_case/case" relevant="/data/returned = '0'"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/>
			<bind nodeset="/data/contact_case/case/create"/>
			<bind nodeset="/data/contact_case/case/create/case_name"/>
			<bind nodeset="/data/contact_case/case/create/owner_id"/>
