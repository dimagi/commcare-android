<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>3. Index Patient Interview</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/701caee60b173d8c28cc98584c00679ffb294d51" uiVersion="1" version="1972" name="3. Index Patient Interview">
					<notEligibleLabel/>
					<ntlpno/>
					<ptserialno/>
					<indexdob/>
					<ptage_years/>
					<childptAge/>
					<subcounty/>
					<parishr/>
					<village/>
					<ptgender/>
					<hiv/>
					<ptparish/>
					<ptvillage/>
					<ptlandlordname/>
					<ptrxspname/>
					<ptrxspphno/>
					<tthhmembers/>
					<hhhead/>
					<householdse>
						<ptoccupation/>
						<spoccupation/>
						<schoollevel/>
						<ablread/>
						<hhIncome/>
						<surehhIncome/>
						<hhExpenditure/>
						<hhExpendituresure/>
						<prtransmode/>
						<spectransmode/>
						<timespentintrans/>
						<moneyspenttrans/>
						<surespenttrans/>
						<indexyrsneighbourhood/>
					</householdse>
					<transmissionqns>
						<weekscoughing/>
						<sureweekscoughing/>
						<ntscurrdwell1mo/>
						<mocurrdwel1year/>
						<knowanytbpt/>
						<anyhhtblst2yrs/>
						<peoplewithtb/>
						<friendswithtb2yrs/>
						<friendswithtb/>
						<roomsinhouse/>
					</transmissionqns>
					<visithm>
						<visithome/>
						<question204/>
						<ptschintvdt/>
						<pthmlandmark/>
						<personalphonenos_group jr:template="">
							<personalphnnos/>
							<phone_no_for_sms/>
						</personalphonenos_group>
					</visithm>
					<last_visit/>
					<invdate/>
					<indexid/>
					<ptage/>
					<age_in_months/>
					<pttype/>
					<ptid/>
					<eligible/>
					<allocation/>
					<allocation_id/>
					<display_case_inf/>
					<group/>
					<uid/>
					<username/>
					<string_data/>
					<new_string_data/>
					<matching_case_id/>
					<old_strata_count/>
					<allocation_block>
						<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id="">
							<update>
								<string_data/>
							</update>
						</case>
					</allocation_block>
					<strata_block>
						<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id="">
							<update>
								<strata_count/>
							</update>
						</case>
					</strata_block>
					<phone_number_count>3</phone_number_count>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><allocation/><allocation_id/><eligible/><ptage/><ptgender/><pthmlandmark/><ptid/><ptparish/><ptrxspphno/><pttype/><ptvillage/><string_data/><subcounty/><username/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance><instance id="casedb" src="jr://instance/casedb"/>
			<instance id="county" src="jr://fixture/item-list:county"/>
			<instance id="subcounty" src="jr://fixture/item-list:subcounty"/>
			<instance id="parish" src="jr://fixture/item-list:parish"/>
			<instance id="commcaresession" src="jr://instance/session"/>
			<instance id="groups" src="jr://fixture/user-groups"/>
			<bind nodeset="/data/notEligibleLabel" constraint="'1'='0'" relevant="not(/data/eligible = 'yes')" required="true()"/>
			<bind nodeset="/data/ntlpno" type="xsd:string" required="true()"/>
			<bind nodeset="/data/ptserialno" type="xsd:string" required="true()"/>
			<bind nodeset="/data/indexdob" type="xsd:date" constraint=".&lt;=today()" jr:constraintMsg="jr:itext('indexdob-constraintMsg')" required="true()"/>
			<bind nodeset="/data/ptage_years" relevant="/data/ptage&gt;2" required="false()"/>
			<bind nodeset="/data/childptAge" relevant="/data/ptage&lt;=2" required="true()"/>
			<bind nodeset="/data/subcounty" required="true()"/>
			<bind nodeset="/data/parishr" required="true()"/>
			<bind nodeset="/data/village" type="xsd:string" required="true()"/>
			<bind nodeset="/data/ptgender" required="true()"/>
			<bind nodeset="/data/hiv" required="true()"/>
			<bind nodeset="/data/ptparish"/>
			<bind nodeset="/data/ptvillage" type="xsd:string"/>
			<bind nodeset="/data/ptlandlordname" type="xsd:string" required="true()"/>
			<bind nodeset="/data/ptrxspname" type="xsd:string" required="false()"/>
			<bind nodeset="/data/ptrxspphno" type="xsd:string" constraint="string-length(.) = 10" jr:constraintMsg="jr:itext('ptrxspphno-constraintMsg')" required="false()"/>
			<bind nodeset="/data/tthhmembers" type="xsd:int" constraint=".&gt;0" jr:constraintMsg="jr:itext('tthhmembers-constraintMsg')" required="true()"/>
			<bind nodeset="/data/hhhead" relevant="/data/ptage &gt;= 18" required="true()"/>
			<bind nodeset="/data/householdse" relevant="/data/hhhead = 'yes'"/>
			<bind nodeset="/data/householdse/ptoccupation" required="true()"/>
			<bind nodeset="/data/householdse/spoccupation" type="xsd:string" relevant="/data/householdse/ptoccupation = 'other'" required="true()"/>
			<bind nodeset="/data/householdse/schoollevel" required="true()"/>
			<bind nodeset="/data/householdse/ablread" required="true()"/>
			<bind nodeset="/data/householdse/hhIncome" type="xsd:int" constraint=".&gt;= 0" jr:constraintMsg="jr:itext('householdse/hhIncome-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/surehhIncome" relevant="/data/householdse/hhIncome &gt; 1000" required="true()"/>
			<bind nodeset="/data/householdse/hhExpenditure" type="xsd:int" constraint=".&gt;0" jr:constraintMsg="jr:itext('householdse/hhExpenditure-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/hhExpendituresure" relevant="/data/householdse/hhExpenditure &gt; 1000" required="true()"/>
			<bind nodeset="/data/householdse/prtransmode" required="true()"/>
			<bind nodeset="/data/householdse/spectransmode" type="xsd:string" relevant="/data/householdse/prtransmode = 'other'" required="true()"/>
			<bind nodeset="/data/householdse/timespentintrans" type="xsd:int" constraint=".&gt;=0 and .&lt;=168" jr:constraintMsg="jr:itext('householdse/timespentintrans-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/moneyspenttrans" type="xsd:int" constraint=". &gt;= 0" jr:constraintMsg="jr:itext('householdse/moneyspenttrans-constraintMsg')" required="true()"/>
			<bind nodeset="/data/householdse/surespenttrans" relevant="/data/householdse/moneyspenttrans &gt; 100" required="true()"/>
			<bind nodeset="/data/householdse/indexyrsneighbourhood" type="xsd:int" constraint=". &lt;= 0 and . &lt;= 120" required="true()"/>
			<bind nodeset="/data/transmissionqns"/>
			<bind nodeset="/data/transmissionqns/weekscoughing" type="xsd:int" constraint=". &lt;= 0" jr:constraintMsg="jr:itext('transmissionqns/weekscoughing-constraintMsg')" required="true()"/>
			<bind nodeset="/data/transmissionqns/sureweekscoughing" relevant="/data/transmissionqns/weekscoughing &gt; 25" required="true()"/>
			<bind nodeset="/data/transmissionqns/ntscurrdwell1mo" type="xsd:int" constraint=".&gt;=0 and .&lt;=31" jr:constraintMsg="jr:itext('ntscurrdwell1mo-constraintMsg')" required="true()"/>
			<bind nodeset="/data/transmissionqns/mocurrdwel1year" type="xsd:int" constraint=".&gt;=0 and .&lt;=12" jr:constraintMsg="jr:itext('mocurrdwel1year-constraintMsg')" required="true()"/>
			<bind nodeset="/data/transmissionqns/knowanytbpt" required="true()"/>
			<bind nodeset="/data/transmissionqns/anyhhtblst2yrs" required="true()"/>
			<bind nodeset="/data/transmissionqns/peoplewithtb" relevant="/data/transmissionqns/anyhhtblst2yrs = 'yes'" required="true()"/>
			<bind nodeset="/data/transmissionqns/friendswithtb2yrs" required="true()"/>
			<bind nodeset="/data/transmissionqns/friendswithtb" relevant="/data/transmissionqns/friendswithtb2yrs = 'yes'" required="true()"/>
			<bind nodeset="/data/transmissionqns/roomsinhouse" type="xsd:int" constraint=".&gt;=1 and .&lt;=30" required="true()"/>
			<bind nodeset="/data/visithm"/>
			<bind nodeset="/data/visithm/visithome" required="true()"/>
			<bind nodeset="/data/visithm/question204" relevant="/data/visithm/visithome = 'yes'" required="true()"/>
			<bind nodeset="/data/visithm/ptschintvdt" type="xsd:date" relevant="/data/visithm/visithome = 'no'" required="true()"/>
			<bind nodeset="/data/visithm/pthmlandmark" type="xsd:string" required="true()"/>
			<bind nodeset="/data/visithm/personalphonenos_group"/>
			<bind nodeset="/data/visithm/personalphonenos_group/personalphnnos" type="xsd:string" constraint="string-length(.) = 10" jr:constraintMsg="jr:itext('personalphnnos-constraintMsg')" required="true()"/>
			<bind nodeset="/data/visithm/personalphonenos_group/phone_no_for_sms" calculate="concat('256', substr(/data/visithm/personalphonenos_group/personalphnnos,1, 10))"/>
			<bind nodeset="/data/last_visit" calculate="today()"/>
			<bind nodeset="/data/invdate"/>
			<bind nodeset="/data/indexid"/>
			<bind nodeset="/data/ptage" calculate="int((today() - date(/data/indexdob)) div 365.25)"/>
			<bind nodeset="/data/age_in_months" calculate="if(/data/ptage &lt;= 2, int((today() - date(/data/indexdob)) div 30.4), '')"/>
			<bind nodeset="/data/pttype" calculate="'index'"/>
			<bind nodeset="/data/ptid"/>
			<bind nodeset="/data/eligible"/>
			<bind nodeset="/data/allocation" calculate="substr(/data/string_data,0,1)"/>
			<bind nodeset="/data/allocation_id" calculate="concat(/data/username, /data/hiv, /data/group, /data/uid, /data/allocation)"/>
			<bind nodeset="/data/display_case_inf"/>
			<bind nodeset="/data/group" calculate="if(/data/ptage&lt;5,1, if (/data/ptage &gt;=5 and /data/ptage &lt;=14,2,3))"/>
			<bind nodeset="/data/uid" calculate="uuid(5)"/>
			<bind nodeset="/data/username" calculate="instance('commcaresession')/session/context/username"/>
			<bind nodeset="/data/string_data" calculate="if(/data/matching_case_id='','',instance('casedb')/casedb/case[@case_id=/data/matching_case_id]/string_data)"/>
			<bind nodeset="/data/new_string_data" calculate="if(/data/string_data = '', '', substr(/data/string_data,1))"/>
			<bind nodeset="/data/matching_case_id" calculate="if(/data/username = '' or /data/hiv = '' or /data/group = '', '',instance('casedb')/casedb/case[@case_type='strata_group'][username = /data/username][int(hiv)=/data/hiv][int(group) = /data/group]/@case_id)"/>
			<bind nodeset="/data/old_strata_count" value="instance('casedb')/casedb/case[@case_type='strata_group'][username = /data/username][int(hiv)=/data/hiv][int(group) = /data/group]/@case_id)"/>
			<bind nodeset="/data/allocation_block"/>
			<bind nodeset="/data/allocation_block/case"/>
			<bind nodeset="/data/allocation_block/case/update"/>
			<bind nodeset="/data/allocation_block/case/update/string_data"/>
			<bind nodeset="/data/strata_block"/>
			<bind nodeset="/data/strata_block/case"/>
			<bind nodeset="/data/strata_block/case/update"/>
			<bind nodeset="/data/strata_block/case/update/strata_count"/><bind nodeset="/data/allocation_block/case/@user_id" calculate="instance('groups')/groups/group/@id"/><bind calculate="/data/meta/timeEnd" nodeset="/data/allocation_block/case/@date_modified" type="xsd:dateTime"/><bind nodeset="/data/allocation_block/case/@case_id" calculate="/data/matching_case_id"/><bind nodeset="/data/allocation_block/case/update/string_data" calculate="/data/new_string_data"/>
			<bind nodeset="/data/phone_number_count"/><bind nodeset="/data/strata_block/case/@user_id" calculate="instance('groups')/groups/group/@id"/><bind calculate="/data/meta/timeEnd" nodeset="/data/strata_block/case/@date_modified" type="xsd:dateTime"/><bind nodeset="/data/strata_block/case/@case_id" calculate="/data/matching_case_id"/><bind nodeset="/data/strata_block/case/update/strata_count" calculate="int(/data/old_strata_count) + 1"/>
			<itext>
				<translation lang="en" default="">
					<text id="notEligibleLabel-label">
						<value>The patient is not eligible to be enrolled in the study. Please return to the home screen.</value>
					</text>
					<text id="ntlpno-label">
						<value>NTLP TB Treatment Number
					</value>
					</text>
					<text id="ptserialno-label">
						<value>Track TB Serial Number </value>
					</text>
					<text id="indexdob-label2">
						<value>Patient Date of Birth</value>
					</text>
					<text id="indexdob-constraintMsg">
						<value>Date cannot be in the future.</value>
					</text>
					<text id="ptage_years-label">
						<value>Patient is <output ref="/data/ptage"/> years old</value>
					</text>
					<text id="childptAge-label">
						<value>Patient is <output ref="/data/age_in_months"/> months old</value>
					</text>
					<text id="subcounty-label">
						<value>Subcounty</value>
					</text>
					<text id="parishr-label">
						<value>Parish</value>
					</text>
					<text id="village-label">
						<value>Village </value>
					</text>
					<text id="ptgender-label">
						<value>Gender</value>
					</text>
					<text id="ptgender-female-label">
						<value>Female</value>
					</text>
					<text id="ptgender-male-label">
						<value>Male</value>
					</text>
					<text id="hiv-label">
						<value>HIV Status</value>
					</text>
					<text id="hiv-0-label">
						<value>Positive</value>
					</text>
					<text id="hiv-1-label">
						<value>Negative</value>
					</text>
					<text id="hiv-2-label">
						<value>Unknown</value>
					</text>
					<text id="hiv-9-label">
						<value>Decline to state</value>
					</text>
					<text id="ptlandlordname-label">
						<value>Name of Landlord</value>
					</text>
					<text id="ptrxspname-label">
						<value>Treatment Supporter Name</value>
					</text>
					<text id="ptrxspphno-label">
						<value>Treatment Supporter Phone Number</value>
					</text>
					<text id="ptrxspphno-constraintMsg">
						<value>Enter a 10 digit phone number.</value>
					</text>
					<text id="tthhmembers-label">
						<value>How many household members are living with the index patient? </value>
					</text>
					<text id="tthhmembers-hint">
						<value>Give the total number of individuals who have slept under same roof as the index patient for more than 5 nights in the last 3 months. Do not include the index patient in the count.</value>
					</text>
					<text id="tthhmembers-constraintMsg">
						<value>The number of household members cannot be less than 1.</value>
					</text>
					<text id="hhhead-label">
						<value>Are you the head of household?</value>
					</text>
					<text id="householdse-label">
						<value>Household SES</value>
					</text>
					<text id="ptoccupation-label">
						<value>What is your occupation?</value>
					</text>
					<text id="ptoccupation-unemployed-label">
						<value>Unemployed</value>
					</text>
					<text id="ptoccupation-student-label">
						<value>Student</value>
					</text>
					<text id="ptoccupation-casual_laborer-label">
						<value>Casual Laborer</value>
					</text>
					<text id="ptoccupation-civil_servant-label">
						<value>Civil Servant</value>
					</text>
					<text id="ptoccupation-ngo_private_sector-label">
						<value>NGO or Private-sector employee</value>
					</text>
					<text id="ptoccupation-housewife-label">
						<value>Housewife</value>
					</text>
					<text id="ptoccupation-domestic_worker-label">
						<value>Domestic Worker</value>
					</text>
					<text id="ptoccupation-self_employed-label">
						<value>Self-employed</value>
					</text>
					<text id="householdse/spoccupation-label">
						<value>Kindly specify which is  your occupation?</value>
					</text>
					<text id="schoollevel-label">
						<value>How far did you go in school?</value>
					</text>
					<text id="schoollevel-nofmsch-label">
						<value>No formal school</value>
					</text>
					<text id="schoollevel-pry1to4-label">
						<value>Primary level 1-4</value>
					</text>
					<text id="schoollevel-pry5to7-label">
						<value>Primary level 5-7</value>
					</text>
					<text id="schoollevel-olevel-label">
						<value>Secondary O level</value>
					</text>
					<text id="schoollevel-alevel-label">
						<value>Secondary A level</value>
					</text>
					<text id="schoollevel-vocatnl-label">
						<value>Vocational</value>
					</text>
					<text id="schoollevel-tertiary-label">
						<value>Tertiary / University</value>
					</text>
					<text id="schoollevel-postgrad-label">
						<value>Post-graduate</value>
					</text>
					<text id="ablwrt-label">
						<value>Can you read  or write a letter or newspaper in any language?</value>
					</text>
					<text id="ablwrt-yes-label">
						<value>Yes</value>
					</text>
					<text id="ablwrt-no-label">
						<value>No</value>
					</text>
					<text id="hhIncome-label">
						<value>What is your weekly household income in thousands of UGX? </value>
					</text>
					<text id="householdse/hhIncome-constraintMsg">
						<value>Income should be greater than zero.</value>
					</text>
					<text id="householdse/surehhIncome-label">
						<value>Are you sure about this reported income (<output value="/data/householdse/hhIncome"/>000 UGX)? Note that weekly income is requested in units of thousands of UGX.</value>
					</text>
					<text id="hhExpenditure-label">
						<value>What are your weekly household expenditures in thousands of UGX? </value>
					</text>
					<text id="householdse/hhExpenditure-hint">
						<value>Remember to include all members of the household.</value>
					</text>
					<text id="householdse/hhExpenditure-constraintMsg">
						<value>Expenditure should be greater than zero</value>
					</text>
					<text id="householdse/hhExpendituresure-label">
						<value>Are you sure about this reported ( <output value="/data/householdse/hhExpenditure"/>000 UGX) weekly household expenditure? Note that household weekly expenditure is requested in units of thousands of UGX.</value>
					</text>
					<text id="householdse/prtransmode-label">
						<value>What is your primary mode of transport to places that are more than 5 minutes from your home?  </value>
					</text>
					<text id="householdse/prtransmode-hint">
						<value>By primary, we mean the mode of transport that you use to travel the greatest distance.</value>
					</text>
					<text id="householdse/prtransmode-walk-label">
						<value>Walking</value>
					</text>
					<text id="householdse/prtransmode-bicycle-label">
						<value>Bicycle</value>
					</text>
					<text id="householdse/prtransmode-bodaboda-label">
						<value>Boda boda </value>
					</text>
					<text id="householdse/prtransmode-taxi-label">
						<value>Taxi/Matatu</value>
					</text>
					<text id="householdse/prtransmode-car-label">
						<value>Car</value>
					</text>
					<text id="householdse/prtransmode-other-label">
						<value>Other</value>
					</text>
					<text id="householdse/spectransmode-label">
						<value>Please specify the other mode of transport</value>
					</text>
					<text id="householdse/timespentintrans-label">
						<value>In a typical week, how much time (in hours) do you spend in transport to places more than 5 minutes from your home?</value>
					</text>
					<text id="householdse/timespentintrans-constraintMsg">
						<value>Hours cannot be less than zero or greater than 168.</value>
					</text>
					<text id="householdse/moneyspenttrans-label">
						<value>In a typical week, how much money do you spend (in thousands of UGX) on transport?</value>
					</text>
					<text id="householdse/moneyspenttrans-constraintMsg">
						<value>Should be greater than or equal to zero.</value>
					</text>
					<text id="householdse/surespenttrans-label">
						<value>Are you sure about this reported (<output value="/data/householdse/moneyspenttrans"/>000 UGX) weekly expenditure on transport? Note that expenditure on transport is requested in units of thousands of UGX.</value>
					</text>
					<text id="householdse/indexyrsneighbourhood-label">
						<value>For how many years have you been living within walking distance of your current dwelling ?</value>
					</text>
					<text id="transmissionqns-label2">
						<value>Transmission Questions</value>
					</text>
					<text id="sypmsdurbfdgns-label">
						<value>How many weeks have you been coughing?</value>
					</text>
					<text id="transmissionqns/weekscoughing-constraintMsg">
						<value>Weeks should greater than or equal to zero.</value>
					</text>
					<text id="transmissionqns/sureweekscoughing-label">
						<value>Are you sure you have been coughing for these (<output value="/data/transmissionqns/weekscoughing"/>) weeks?</value>
					</text>
					<text id="ntscurrdwell1mo-label">
						<value>How many nights did you sleep in your current dwelling over the past one month?</value>
					</text>
					<text id="ntscurrdwell1mo-constraintMsg">
						<value>Night must be less than or equal to 31.</value>
					</text>
					<text id="mocurrdwel1year-label">
						<value>How many months did you sleep in that dwelling over the past 12 months?</value>
					</text>
					<text id="mocurrdwel1year-constraintMsg">
						<value>Months must be less than or equal to 12.</value>
					</text>
					<text id="knowanytbpt-label">
						<value>Do you know anyone else who currently has TB?</value>
					</text>
					<text id="anyhhtblst2yrs-label">
						<value> In the past 2 years, have you shared a dwelling with anyone who had active TB while you were living together??</value>
					</text>
					<text id="transmissionqns/anyhhtblst2yrs-hint">
						<value>Sharing a dwelling is defined as spending 5 or more nights sleeping under the same roof at the time</value>
					</text>
					<text id="transmissionqns/peoplewithtb-label">
						<value>Then if yes: Was the person (or people) who had TB an adult or a child under 15 years?  (with option for ""both"")</value>
