    android:title="Developer Mode Enabled" 
    android:entryValues="@array/pref_enabled_vals" 
    android:entries="@array/pref_enabled_lables"/>
<ListPreference 
    android:key="cc-grid-menus" 
    android:enabled="true" 
    android:title="Grid Menus Enables" 
